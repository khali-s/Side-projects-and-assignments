    public int whichBox(int y, int x){
    //x-->0,1,2 
    int boxV= y/this.SIZE;
    int boxH = x/this.SIZE;
    int num = boxV*this.SIZE + boxH;
    return num;
    }
 /*   public int[] nextCoord(int y, int x){
